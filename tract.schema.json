{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TractJSON",
  "type": "object",
  "required": [
    "diameter",
    "glottis",
    "junctionOutputRight",
    "justionOutputLeft",
    "left",
    "maxAmplitude",
    "newReflection",
    "newReflectionLeft",
    "newReflectionNose",
    "newReflectionRight",
    "noseDiameter",
    "noseJunctionOutputLeft",
    "noseJunctionOutputRight",
    "noseLeft",
    "noseMaxAmplitude",
    "noseReflection",
    "noseRight",
    "reflection",
    "reflectionLeft",
    "reflectionNose",
    "reflectionRight",
    "right",
    "sampleCount",
    "sampleRate",
    "time",
    "transients",
    "turbulencePoints"
  ],
  "properties": {
    "diameter": {
      "description": "vocal tract cell diameters",
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "glottis": {
      "$ref": "#/definitions/GlottisJSON"
    },
    "junctionOutputRight": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "justionOutputLeft": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 17,
      "minItems": 17
    },
    "left": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "maxAmplitude": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "newReflection": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "newReflectionLeft": {
      "type": "number",
      "format": "double"
    },
    "newReflectionNose": {
      "type": "number",
      "format": "double"
    },
    "newReflectionRight": {
      "type": "number",
      "format": "double"
    },
    "noseDiameter": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "noseJunctionOutputLeft": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 13,
      "minItems": 13
    },
    "noseJunctionOutputRight": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "noseLeft": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "noseMaxAmplitude": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "noseReflection": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "noseRight": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 12,
      "minItems": 12
    },
    "reflection": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "reflectionLeft": {
      "type": "number",
      "format": "double"
    },
    "reflectionNose": {
      "type": "number",
      "format": "double"
    },
    "reflectionRight": {
      "type": "number",
      "format": "double"
    },
    "right": {
      "type": "array",
      "items": {
        "type": "number",
        "format": "double"
      },
      "maxItems": 16,
      "minItems": 16
    },
    "sampleCount": {
      "type": "integer",
      "format": "uint",
      "minimum": 0.0
    },
    "sampleRate": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "time": {
      "type": "number",
      "format": "float"
    },
    "transients": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TransientJSON"
      }
    },
    "turbulencePoints": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TurbulencePointJSON"
      }
    }
  },
  "definitions": {
    "GlottisJSON": {
      "type": "object",
      "required": [
        "alpha",
        "alwaysVoice",
        "autoWobble",
        "delta",
        "e0",
        "epsilon",
        "intensity",
        "isTouched",
        "loudness",
        "newFrequency",
        "newTenseness",
        "noiseGenerator",
        "oldFrequency",
        "oldTenseness",
        "omega",
        "random",
        "sampleCount",
        "sampleRate",
        "seed",
        "shift",
        "smoothFrequency",
        "targetFrequency",
        "targetTenseness",
        "te",
        "timeInWaveform",
        "vibratoAmount",
        "vibratoFrequency",
        "waveformLength"
      ],
      "properties": {
        "alpha": {
          "type": "number",
          "format": "float"
        },
        "alwaysVoice": {
          "type": "boolean"
        },
        "autoWobble": {
          "type": "boolean"
        },
        "delta": {
          "type": "number",
          "format": "float"
        },
        "e0": {
          "type": "number",
          "format": "float"
        },
        "epsilon": {
          "type": "number",
          "format": "float"
        },
        "intensity": {
          "type": "number",
          "format": "float"
        },
        "isTouched": {
          "type": "boolean"
        },
        "loudness": {
          "type": "number",
          "format": "float"
        },
        "newFrequency": {
          "type": "number",
          "format": "float"
        },
        "newTenseness": {
          "type": "number",
          "format": "float"
        },
        "noiseGenerator": {
          "$ref": "#/definitions/NoiseGeneratorJSON"
        },
        "oldFrequency": {
          "type": "number",
          "format": "float"
        },
        "oldTenseness": {
          "type": "number",
          "format": "float"
        },
        "omega": {
          "type": "number",
          "format": "float"
        },
        "random": {
          "anyOf": [
            {
              "$ref": "#/definitions/ThreadRngJSON"
            },
            {
              "type": "null"
            }
          ]
        },
        "sampleCount": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "sampleRate": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "seed": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0.0
        },
        "shift": {
          "type": "number",
          "format": "float"
        },
        "smoothFrequency": {
          "type": "number",
          "format": "float"
        },
        "targetFrequency": {
          "type": "number",
          "format": "float"
        },
        "targetTenseness": {
          "type": "number",
          "format": "float"
        },
        "te": {
          "type": "number",
          "format": "float"
        },
        "timeInWaveform": {
          "type": "number",
          "format": "float"
        },
        "vibratoAmount": {
          "type": "number",
          "format": "float"
        },
        "vibratoFrequency": {
          "type": "number",
          "format": "float"
        },
        "waveformLength": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "GradJSON": {
      "type": "object",
      "required": [
        "x",
        "y",
        "z"
      ],
      "properties": {
        "x": {
          "type": "number",
          "format": "float"
        },
        "y": {
          "type": "number",
          "format": "float"
        },
        "z": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "NoiseGeneratorJSON": {
      "type": "object",
      "required": [
        "gradP",
        "perm"
      ],
      "properties": {
        "gradP": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/GradJSON"
          },
          "maxItems": 16,
          "minItems": 16
        },
        "perm": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "uint8",
            "minimum": 0.0
          },
          "maxItems": 16,
          "minItems": 16
        }
      }
    },
    "ThreadRngJSON": {
      "type": "object",
      "required": [
        "randoms"
      ],
      "properties": {
        "randoms": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "double"
          }
        }
      }
    },
    "TransientJSON": {
      "type": "object",
      "required": [
        "exponent",
        "lifeTime",
        "position",
        "startTime",
        "strength"
      ],
      "properties": {
        "exponent": {
          "type": "number",
          "format": "double"
        },
        "lifeTime": {
          "type": "number",
          "format": "float"
        },
        "position": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "startTime": {
          "type": "number",
          "format": "float"
        },
        "strength": {
          "type": "number",
          "format": "double"
        }
      }
    },
    "TurbulencePointJSON": {
      "type": "object",
      "required": [
        "diameter",
        "endTime",
        "position",
        "startTime"
      ],
      "properties": {
        "diameter": {
          "type": "number",
          "format": "float"
        },
        "endTime": {
          "type": "number",
          "format": "float"
        },
        "position": {
          "type": "number",
          "format": "float"
        },
        "startTime": {
          "type": "number",
          "format": "float"
        }
      }
    }
  }
}